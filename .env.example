# outside of docker, we don't use nsjail
ADMYRAL_DISABLE_NSJAIL=true

# used for generating migrations
MIGRATION_DATABASE_URL=sqlite+aiosqlite:///.admyral/migration.db

# Optional but required if ai_action action is used
OPENAI_API_KEY=""
# Optional but required if send_email action is used
RESEND_API_KEY=""
RESEND_EMAIL=""

# used for development to deactivate telemetry
ADMYRAL_ENV="dev"

# Telemetry
ADMYRAL_DISABLE_TELEMETRY=false
